{
  "name": "gatsby-source-kintone",
  "version": "0.2.0",
  "description": "A Gatsby plugin for kintone",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "release": "yarn lint && yarn build && standard-version",
    "release:dryrun": "yarn lint && yarn build && standard-version --dry-run",
    "start": "tsc --watch",
    "lint:eslint": "eslint src/*.ts",
    "lint:tsc": "tsc --noEmit",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/koba04/gatsby-source-kintone.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "kintone"
  ],
  "author": "koba04",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koba04/gatsby-source-kintone/issues"
  },
  "homepage": "https://github.com/koba04/gatsby-source-kintone#readme",
  "devDependencies": {
    "@cybozu/eslint-config": "^10.0.2",
    "eslint": "^6.8.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@kintone/rest-api-client": "^0.2.0"
  }
}
